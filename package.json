{
  "$schema": "https://json.schemastore.org/package",
  "name": "@mark1russell7/impl-mcp-dev",
  "version": "0.0.1",
  "description": "Ready-to-use MCP server exposing the procedure ecosystem to Claude",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mcp-server": "./bin/mcp-server.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo"
  },
  "sideEffects": false,
  "dependencies": {
    "@mark1russell7/client": "github:mark1russell7/client#main",
    "@mark1russell7/client-mcp": "github:mark1russell7/client-mcp#main",
    "@mark1russell7/bundle-dev": "github:mark1russell7/bundle-dev#main"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3",
    "@mark1russell7/cue": "github:mark1russell7/cue#main"
  }
}
